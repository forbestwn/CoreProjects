<entity name="data.datatypedef" description="this is used to defined data type basic info" class="com.nosliw.data.datatype.importer.HAPDataTypeImpLoad">

	<property name="description" dataType="string" description="data type description">
		<dbColumnInfos>
			<dbColumnInfo definition="VARCHAR(20)"/>
		</dbColumnInfos>
	</property>

	<property name="info" dataType="object" subDataType="com.nosliw.data.datatype.importer.HAPDataTypeInfoImp" description="data type info">
		<dbColumnInfos>
			<dbColumnInfo column="id" definition="VARCHAR(20) NOT NULL PRIMARY KEY"/>
			<dbColumnInfo column="name" definition="VARCHAR(20) NOT NULL" />
			<dbColumnInfo column="version" definition="VARCHAR(20) NOT NULL"/>
			<dbColumnInfo column="versionMajor" definition="INT" setter="no"/>
			<dbColumnInfo column="versionMinor" definition="INT" setter="no"/>
			<dbColumnInfo column="versionRevision" type="string" definition="VARCHAR(20)" setter="no"/>
		</dbColumnInfos>
	</property>

	<property name="parentInfo" dataType="object" subDataType="com.nosliw.data.datatype.importer.HAPDataTypeInfoImp" description="parent data type"/>

	<property name="linkedVersion" dataType="object" subDataType="com.nosliw.data.datatype.importer.HAPDataTypeVersionImp" description="lined data type in the same family"/>

	<property name="operations" type="list" elementTag="operation">
		<child reference="data.operation"/>
	</property>
</entity>
