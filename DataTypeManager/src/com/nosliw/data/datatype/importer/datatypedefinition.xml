<entity name="data.datatypedef" class="com.nosliw.data.datatype.importer.HAPDataTypeImpLoad" table="nosliw.DATATYPE" description="this is used to defined data type basic info">
	<property name="description" dataType="string" description="data type description">
		<dbColumnInfos>
			<dbColumnInfo definition="VARCHAR(20)"/>
		</dbColumnInfos>
	</property>

	<property name="name" dataType="object" subDataType="com.nosliw.data.datatype.importer.HAPDataTypeInfoImp" description="data type info">
		<dbColumnInfos pathType="propertyAsPath">
			<dbColumnInfo column="id" definition="VARCHAR(20) NOT NULL PRIMARY KEY" getter="getFullName" setter="setFullName"/>
			<dbColumnInfo column="name" definition="VARCHAR(20) NOT NULL" setter="no"/>
			<dbColumnInfo column="versionFullName" definition="VARCHAR(20) NOT NULL" setter="no"/>
			<dbColumnInfo column="versionMajor" definition="INT" setter="no"/>
			<dbColumnInfo column="versionMinor" definition="INT" setter="no"/>
			<dbColumnInfo column="versionRevision" definition="VARCHAR(20)" setter="no"/>
		</dbColumnInfos>
	</property>

	<property name="parentInfo" dataType="object" subDataType="com.nosliw.data.datatype.importer.HAPDataTypeInfoImp" description="parent data type">
		<dbColumnInfos pathType="propertyAsPath">
			<dbColumnInfo definition="VARCHAR(20)" getter="getFullName" setter="setFullName"/>
		</dbColumnInfos>
	</property>

	<property name="linkedVersion" dataType="object" subDataType="com.nosliw.data.datatype.importer.HAPDataTypeVersionImp" description="lined data type in the same family">
		<dbColumnInfos>
			<dbColumnInfo definition="VARCHAR(20)" setter="setName"/>
		</dbColumnInfos>
	</property>

	<property name="operations" type="list" elementTag="operation">
		<child reference="data.operation"/>
	</property>
</entity>
